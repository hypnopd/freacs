%h1=t('.administrators')
=link_to t(".new_admin"), new_admin_registration_path

#list
  -if @admins
    %table
      %thead
        %tr
          %th=t('.email')
          %th=t('.actions')
      %tbody
        - @admins.each do |admin|
          %tr
            %td= link_to admin.email, admin_admin_path(admin), :class => "email"
            %td= link_to t('edit'), edit_admin_registration_path(admin), :class => "edit"